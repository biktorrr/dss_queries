PREFIX dss: 	<http://purl.org/collections/nl/dss/>
PREFIX mdb:	<http://purl.org/collections/nl/dss/mdb/>

SELECT ?s ?lab WHERE 
{
	?s rdf:type dss:Ship.
	?s rdfs:label ?lab.
	FILTER (str(?lab) = "Johanna")
}
LIMIT 10
